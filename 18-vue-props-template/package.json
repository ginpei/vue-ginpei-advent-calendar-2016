{
  "private": "true",
  "scripts": {
    "start": "npm run vue-watch",
    "vue-build": "browserify -t vueify -e src/main.js -o out/app.js ",
    "vue-lint": "eslint src/*",
    "vue-watch": "chokidar src/* -c 'npm run vue-lint && npm run vue-build && echo OK \\(`date`\\)'"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "browserify": "^13.1.1",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.11.1",
    "eslint-config-vue": "^2.0.1",
    "eslint-plugin-vue": "^1.0.0",
    "vue": "^2.1.4",
    "vue-props-template": "https://github.com/ginpei/vue-props-template.git",
    "vueify": "^9.3.0",
    "vuex": "^2.0.0"
  },
  "devDependencies": {}
}
